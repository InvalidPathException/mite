struct coord {
    x: i32,
    y: i32
}

struct locations {
    home: coord,
    work: coord
}

export fn main(): i32 {
    let home = coord {
        x: 3,
        y: 4
    };
    let work = coord {
        x: 5,
        y: 6
    };
    let l = locations { home, work };
    l.work.y = 8;
    return l.home.x + l.home.y + l.work.x + l.work.y;
}
