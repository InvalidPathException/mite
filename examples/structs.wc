struct coord {
    x: i32,
    y: i32
}

struct locations {
    home: coord,
    work: coord
}

export fn main(): i32 {
    let home: coord;
    home.x = 3;
    home.y = 4;
    let work: coord;
    work.x = 5;
    work.y = 6;
    let l: locations;
    l.home = home;
    l.work = work;
    l.work.y = 8;
    return l.home.x + l.home.y + l.work.x + l.work.y;
}
